{
  "Server": {
    "Folder": "{path/to/server/folder}",
    "FileName": "{server.jar}",
    "Protocol": "755",
    "Version": "1.17"
  },
  "Commands": {
    "StartServer": "java \u003cCommands.StartServerParam\u003e -jar \u003cServer.FileName\u003e nogui",
    "StartServerParam": "-Xmx1024M -Xms1024M",
    "StopServer": "stop",
    "StopServerAllowKill": 10
  },
  "Msh": {
    "CheckForUpdates": true,
    "Debug": false,
    "InfoHibernation": "                   \u0026fserver status:\n                   \u0026b\u0026lHIBERNATING",
    "InfoStarting": "                   \u0026fserver status:\n                    \u00266\u0026lWARMING UP",
    "Port": "25555",
    "TimeBeforeStoppingEmptyServer": 60
  }
}